{
    "title": "Note Book",
    "plugins": ["katex", "github", "anchor-navigation-ex", "github-buttons", "tbfed-pagefooter", "donate", "toggle-chapters"],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/lowzj/note-book"
        },
        "github-buttons": {
            "repo": "lowzj/note-book",
            "types": [
                "star"
            ],
            "size": "small"
        },
        "anchor-navigation-ex": {
            "isRewritePageTitle": false,
            "showLevel": false
        },
        "donate": {
            "wechat": "./img/donate-wechat.png",
            "alipay": "./img/donate-alipay.png",
            "button": "赏",
            "alipayText": "Alipay ¥1",
            "wechatText": "Wechat ¥1"
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy lowzj 2017",
            "modify_label": "Modified: ",
            "modify_format": "YYYY-MM-DD HH:mm"
        }
    }
}
